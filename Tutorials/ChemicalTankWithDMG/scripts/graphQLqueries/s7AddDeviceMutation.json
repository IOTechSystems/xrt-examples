{
  "query": "mutation AddDevice($xrtInstanceID: ID!, $input: CreateDevice!) { deviceAdd(xrtInstanceID: $xrtInstanceID, input: $input) }",
  "variables": {
    "xrtInstanceID": "XrtChemicalTankInstance",
    "input": {
      "device": "S7-Server",
      "device_info": {
        "profileName": "Server",
        "protocols": {
          "S7": {
            "IP": "${S7_SIM_ADDRESS}",
            "Rack": 0,
            "Slot": 2
          }
        }
      }
    }
  }
}