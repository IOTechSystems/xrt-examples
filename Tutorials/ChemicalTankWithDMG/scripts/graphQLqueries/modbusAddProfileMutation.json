{
  "query": "mutation AddProfile($xrtInstanceID: ID!, $input: CreateProfile!, $deviceService: String!) { profileAdd(xrtInstanceID: $xrtInstanceID, input: $input, deviceService: $deviceService) }",
  "variables": {
    "xrtInstanceID": "XrtChemicalTankInstance",
    "deviceService": "modbus_tcp",
    "input": {
      "profile": {
        "name": "modbus-sim-profile",
        "description": "Chemical Tank",
        "labels": [
          "modbus"
        ],
        "deviceResources": [
          {
            "name": "Temperature",
            "description": "Temperature of liquid within the tank",
            "attributes": {
              "primaryTable": "HOLDING_REGISTERS",
              "startingAddress": 4001
            },
            "properties": {
              "valueType": "INT16",
              "readWrite": "RW"
            }
          },
          {
            "name": "Pressure",
            "description": "Pressure within the tank",
            "attributes": {
              "primaryTable": "HOLDING_REGISTERS",
              "startingAddress": 4003
            },
            "properties": {
              "valueType": "INT16",
              "readWrite": "RW"
            }
          },
          {
            "name": "Level",
            "description": "Level within the tank",
            "attributes": {
              "primaryTable": "HOLDING_REGISTERS",
              "startingAddress": 4017
            },
            "properties": {
              "valueType": "INT16",
              "readWrite": "RW"
            }
          }
        ]
      }
    }
  }
}