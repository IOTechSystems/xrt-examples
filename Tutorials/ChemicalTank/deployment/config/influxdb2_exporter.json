{
  "Library": "libxrt-influxdb-exporter.so",
  "Factory": "xrt_influxdb_exporter_factory",
  "InfluxVersion": 2,
  "ServerURI": "${XRT_INFLUXDB_SERVICE}",
  "Bucket"  : "${XRT_INFLUXDB_BUCKET}",
  "Org": "${XRT_INFLUXDB_ORG}",
  "Token": "${XRT_INFLUXDB_TOKEN}",
  "Measurement": "ResourceName",
  "SelectData": [
    { "Pattern": "xrt/devices/#", "Format": "Raw"}
  ],
  "BatchConfig":
  {
    "BatchSize": 5,
    "BatchTimeout": 5000,
    "Scheduler":"sched",
    "ThreadPool":"pool"
  },
  "Bus": "bus",
  "Logger": "logger",
  "ConnectTimeout": 4000,
  "Timeout": 1500
}
