{
  "Library": "libxrt-modbus-device-service.so",
  "Factory": "xrt_modbus_device_service_factory",
  "Name": "modbus_tcp",
  "ProfileDir": "${XRT_MODBUS_TCP_PROFILE_DIR}",
  "StateDir": "${XRT_MODBUS_TCP_STATE_DIR}",
  "Scheduler": "sched",
  "Logger": "logger",
  "ThreadPool": "pool",
  "Bus": "bus",
  "PublishSource": true,
  "DeviceDownTimeout": 60,
  "AllowedFails": 5,
  "TelemetryTopic": "xrt/${XRT_SERVER_ID}/devices/modbus_tcp/telemetry",
  "DiscoveryTopic": "xrt/${XRT_SERVER_ID}/devices/modbus_tcp/discovery",
  "RequestTopic": "xrt/${XRT_SERVER_ID}/devices/modbus_tcp/request",
  "ReplyTopic": "xrt/${XRT_SERVER_ID}/devices/modbus_tcp/reply",
  "StatusTopic": "xrt/${XRT_SERVER_ID}/devices/modbus_tcp/status",
  "EventTopic": "xrt/${XRT_SERVER_ID}/devices/modbus_tcp/event"
}
