{
  "Library":"libxrt-virtual-device-service.so",
  "Factory":"xrt_virtual_device_service_factory",
  "Name":"virtual_device_service",
  "Topic":"virtual_device_service/data",
  "Devices":{
     "Random-Device1":{
        "profile":"device-virtual",
        "protocols":{
           "Other":{
              "Address":"device-virtual-01"
           }
        }
     },
     "Random-Device2":{
        "profile":"device-virtual2",
        "protocols":{
           "Other":{
              "Address":"device-virtual-01"
           }
        }
     }
  },
  "Schedules":[
     {
        "device":"Random-Device1",
        "resource":"RandomInt8",
        "interval":2000000
     },
     {
        "device":"Random-Device2",
        "resource":"RandomInt8",
        "interval":4000000
     }
  ],
  "ProfileDir":"config/profiles",
  "Scheduler":"sched",
  "Logger":"logger_info",
  "ThreadPool":"pool",
  "Bus":"bus"
}