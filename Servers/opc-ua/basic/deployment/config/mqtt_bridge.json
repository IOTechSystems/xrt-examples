{
    "Library": "libxrt-mqtt-bridge.so",
    "Factory": "xrt_mqtt_bridge_factory",
    "Bus": "bus",
    "Patterns": [{"Pattern": "xrt/devices/s7/reply", "Retain": false, "Qos": 1},
                 {"Pattern": "xrt/devices/s7/discovery", "Retain": false, "Qos": 1},
                 {"Pattern": "xrt/devices/s7/telemetry", "Retain": false, "Qos": 1},
                 {"Pattern": "xrt/devices/s7/event", "Retain": false, "Qos": 1}],
    "MQTTPatterns": [{"Pattern": "xrt/devices/s7/request", "Sync": true}],
    "MQTTConfig": {
        "ServerURI": "${XRT_MQTT_BROKER}",
        "ClientID": "xrt",
        "ClientConfig": {
          "KeepAliveInterval": 60,
          "MQTTVersion": 5,
          "RetryInterval": 5,
          "Username": "${XRT_MQTT_USERNAME}",
          "Password": "${XRT_MQTT_PASSWORD}",
          "MaxBufferedMessages": 100
        }
    },
    "Logger": "logger",
    "QueueSize": 100,
    "Cookie": 125
}
