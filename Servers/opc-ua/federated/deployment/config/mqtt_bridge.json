{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "Patterns": [ 
    "xrt/request",
    "+/devices/+/request"
  ],
  "MQTTPatterns": [ 
    "xrt/reply",
    "xrt/discovery",
    "+/devices/+/telemetry",
    "+/devices/+/reply",
    "+/devices/+/event"
  ],
  "MQTTConfig":
  {
    "ServerURI": "${XRT_MQTT_BROKER}",
    "ClientID": "xrt-opc-ua-server",
    "QoS": 1,
    "ClientConfig":
    {
      "KeepAliveInterval": 60,
      "MQTTVersion": 5,
      "RetryInterval": 5,
      "Username": "${XRT_MQTT_USERNAME}",
      "Password": "${XRT_MQTT_PASSWORD}",
      "MaxBufferedMessages": 100
    }
  },
  "Cookie": 125,
  "QueueSize": 5,
  "Bus": "bus",
  "Logger": "logger"
}
