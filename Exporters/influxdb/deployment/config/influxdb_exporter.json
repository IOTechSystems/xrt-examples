{
  "Library": "libxrt-influxdb-exporter.so",
  "Factory": "xrt_influxdb_exporter_factory",
  "ServerURI": "${XRT_INFLUXDB_SERVICE}",
  "DBName" : "db1",
  "AuthEnabled": true,
  "UserName": "${XRT_INFLUXDB_USERNAME}",
  "Password": "${XRT_INFLUXDB_PASSWORD}",
  "SelectData": [
    { "Pattern": "device/#", "Format": "Device"}
  ],
  "Bus": "bus",
  "Scheduler":"sched",
  "ThreadPool":"pool",
  "Logger": "logger_debug",
  "BatchSize": 5,
  "ConnectTimeout": 4000,
  "Timeout": 1500
}
