{
  "Library": "libxrt-influxdb-exporter.so",
  "Factory": "xrt_influxdb_exporter_factory",
  "InfluxVersion": 2,
  "ServerURI": "${XRT_INFLUXDB_SERVICE}",
  "Bucket"  : "${XRT_INFLUXDB_BUCKET}",
  "Org": "${XRT_INFLUXDB_ORG}",
  "Token": "${XRT_INFLUXDB_TOKEN}",
  "Measurement": "DeviceName",
  "Tags": ["TagDeviceName", "TagScheduleName"],
  "SelectData": [
    { "Pattern": "spBv1.0/${SPARKPLUG_GROUP}/NDATA/${SPARKPLUG_NODE}", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"]}
  ],
  "BatchConfig":
  {
    "BatchSize": 5,
    "BatchTimeout": 5000,
    "Scheduler":"sched",
    "ThreadPool":"pool"
  },
  "Bus": "bus",
  "Logger": "logger_debug",
  "ConnectTimeout": 4000,
  "Timeout": 1500
}
