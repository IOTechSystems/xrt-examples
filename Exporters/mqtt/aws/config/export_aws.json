{
  "Library": "libxrt-mqtt-exporter.so",
  "Factory": "xrt_mqtt_exporter_factory",
  "MQTTConfig":
  {
    "ServerURI": "${MQTT_EXPORTER_AWS_SERVER_URI}",
    "ClientID": "${MQTT_EXPORTER_AWS_CLIENT_ID}",
    "MQTTInstance": 0,
    "QoS": 1,
    "ClientConfig":
    {
      "KeepAliveInterval": 10,
      "MQTTVersion": 4,
      "Reliable": true,
      "ConnectTimeout": 30,
      "CleanSession":true,
      "SSLConfig":
      {
        "EnableServerCertAuth": true,
        "SSLVersion": 3,
        "TrustStore": "${MQTT_EXPORTER_AWS_TRUST_STORE}",
        "KeyStore": "${MQTT_EXPORTER_AWS_KEY_STORE}",
        "PrivateKey": "${MQTT_EXPORTER_AWS_PRIVATE_KEY}",
        "EnabledCipherSuites" : "ALL"
      }
    }
  },
  "Bus": "bus",
  "Logger": "logger_debug",
  "Pattern": "device/#",
  "Topic": "test/data",
  "QueueSize": 4
}
