{
  "Library": "libxrt-bacnet-mstp-device-service.so",
  "Factory": "xrt_bacnet_mstp_device_service_factory",
  "Name": "bacnet_mstp",
  "TelemetryTopic": "xrt/devices/bacnet_mstp/telemetry",
  "RequestTopic": "xrt/devices/bacnet_mstp/request",
  "ReplyTopic": "xrt/devices/bacnet_mstp/reply",
  "DiscoveryTopic": "xrt/devices/bacnet_mstp/discovery",
  "StatusTopic": "xrt/devices/bacnet_mstp/status",
  "AutoRegister": false,
  "DiscoveryInterval": 0,
  "PublishRegisteredDevices": true,
  "PublishAttributes": true,
  "ProfileDir": "./deployment/profiles",
  "StateDir": "./deployment/state",
  "Scheduler": "sched",
  "Logger": "logger",
  "ThreadPool": "pool",
  "Bus": "bus",
  "DeviceDownTimeout": 60,
  "AllowedFails": 5,
  "Driver": {
    "DeviceObject": {
      "InstanceID": 0
    },
    "SerialInterface": "/tmp/virtualport",
    "APDUTimeout": 10000,
    "APDURetries": 3,
    "MultiBatchSize": 20,
    "MultiRead": true,
    "MultiWrite": true,
    "CancelStaleCOV": true,
    "DiscoverMode": "All"
  }
}
