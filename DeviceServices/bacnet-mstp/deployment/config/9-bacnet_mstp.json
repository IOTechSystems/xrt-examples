{
  "Library": "libxrt-bacnet-mstp-device-service.so",
  "Factory": "xrt_bacnet_mstp_device_service_factory",
  "Name": "bacnet_mstp",
  "AutoRegister": false,
  "DiscoveryInterval": 0,
  "PublishRegisteredDevices": true,
  "PublishAttributes": true,
  "ProfileDir": "${XRT_STATE_DIR}/profiles",
  "StateDir": "${XRT_STATE_DIR}",
  "Group":"${SPARKPLUG_GROUP}",
  "Scheduler": "5-sched",
  "Logger": "2-logger",
  "ThreadPool": "3-pool",
  "Bus": "6-bus",
  "DeviceDownTimeout": 60,
  "AllowedFails": 5,
  "Driver": {
    "DeviceObject": {
      "InstanceID": 0
    },
    "SerialInterface": "/tmp/virtualport",
    "APDUTimeout": 10000,
    "APDURetries": 3,
    "MultiBatchSize": 20,
    "MultiRead": true,
    "MultiWrite": true,
    "CancelStaleCOV": true,
    "DiscoverMode": "All"
  }
}
