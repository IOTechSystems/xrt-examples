{
  "Library": "libxrt-ethercat-device-service.so",
  "Factory": "xrt_ethercat_device_service_factory",
  "Name": "ethercat",
  "ProfileDir": "${XRT_STATE_DIR}/profiles",
  "StateDir": "${XRT_STATE_DIR}",
  "Group":"${SPARKPLUG_GROUP}",
  "Scheduler": "5-sched",
  "SchedulesThreadPool": "4-devpool",
  "Logger": "11-logger_debug",
  "ThreadPool": "3-pool",
  "Bus": "6-bus",
  "DeviceDownTimeout": 60,
  "AllowedFails": 5,
  "Driver": {
    "NetworkInterface1": "eno1",
    "CycleTime_us": 4000,
    "FinalState": "OP"
  }
}
