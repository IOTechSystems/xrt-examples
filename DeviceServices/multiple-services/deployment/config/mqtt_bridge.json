{
    "Library": "libxrt-mqtt-bridge.so",
    "Factory": "xrt_mqtt_bridge_factory",
    "Patterns": [{"Pattern":"xrt/devices/modbus/telemetry","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/modbus/reply","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/modbus/request","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/bacnet_ip/telemetry","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/bacnet_ip/reply","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/bacnet_ip/request","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/bacnet_ip/discovery","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/opc_ua/telemetry","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/opc_ua/reply","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/opc_ua/request","Retain": false, "Qos": 1},
                 {"Pattern":"xrt/devices/opc_ua/discovery","Retain": false, "Qos": 1} 
    ],
    "MQTTPatterns": [{"Pattern":"xrt/devices/modbus/request", "Sync": true},
                     {"Pattern":"xrt/devices/bacnet_ip/request", "Sync": true},
                     {"Pattern":"xrt/devices/opc_ua/request", "Sync": true}
    ],
    "MQTTConfig": {
        "ServerURI": "${XRT_MQTT_BROKER}",
        "ClientID": "xrt",
        "QoS": 1,
        "ClientConfig": {
            "KeepAliveInterval": 60,
            "MQTTVersion": 5,
            "Reliable": true,
            "RetryInterval": 5,
            "Username": "",
            "Password": ""
        }
    },
    "Cookie": 125,
    "QueueSize": 5,
    "Bus": "bus",
    "Logger": "logger"
}
