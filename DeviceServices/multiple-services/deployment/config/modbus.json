{
    "Library": "libxrt-modbus-device-service.so",
    "Factory": "xrt_modbus_device_service_factory",
    "Name": "modbus",
    "ProfileDir": "${DIR_PATH}/modbus-tcp/deployment/profiles",
    "StateDir": "${DIR_PATH}/modbus-tcp/deployment/state",
    "Scheduler": "sched",
    "Logger": "logger",
    "ThreadPool": "pool",
    "Bus": "bus",
    "DeviceDownTimeout": 60,
    "AllowedFails": 5,
    "TelemetryTopic": "xrt/devices/modbus/telemetry",
    "RequestTopic": "xrt/devices/modbus/request",
    "ReplyTopic": "xrt/devices/modbus/reply",
    "StatusTopic": "xrt/devices/modbus/status"
}