{
    "Library": "libxrt-bacnet-ip-device-service.so",
    "Factory": "xrt_bacnet_ip_device_service_factory",
    "Name": "bacnet_ip",
    "TelemetryTopic": "xrt/devices/bacnet_ip/telemetry",
    "RequestTopic": "xrt/devices/bacnet_ip/request",
    "ReplyTopic": "xrt/devices/bacnet_ip/reply",
    "DiscoveryTopic": "xrt/devices/bacnet_ip/discovery",
    "AutoRegister": false,
    "DiscoveryInterval": 0,
    "PublishRegisteredDevices": true,
    "PublishAttributes": true,
    "PublishServerId": true,
    "ProfileDir": "${DIR_PATH}/bacnet-ip/deployment/bacnet-ip-profiles",
    "StateDir": "${DIR_PATH}/bacnet-ip/deployment/bacnet-ip-state",
    "Scheduler": "sched",
    "Logger": "logger",
    "ThreadPool": "pool",
    "Bus": "bus",
    "Driver": {
        "DeviceObject": {
            "InstanceID": 0
        },
        "Port": 47808,
        "APDUTimeout": 3000,
        "APDURetries": 3,
        "MultiBatchSize": 20,
        "MultiRead": true,
        "MultiWrite": true,
        "CancelStaleCOV": true,
        "DiscoverMode": "All",
        "BBMDAddress": "${BACNET_IP_SIM_ADDRESS}",
        "BBMDPort": 47808
    }
}