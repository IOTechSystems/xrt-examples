{
  "Library": "libxrt-opcua-device-service.so",
  "Factory": "xrt_opcua_device_service_factory",
  "Name": "opc_ua",
  "AutoRegister": false,
  "DiscoveryInterval": 300,
  "PublishRegisteredDevices": true,
  "PublishAttributes": true,
  "ProfileDir": "${DIR_PATH}/opc-ua/deployment/profiles",
  "StateDir": "${DIR_PATH}/opc-ua/deployment/state",
  "Group":"${SPARKPLUG_GROUP}",
  "Scheduler": "sched",
  "Logger": "logger",
  "ThreadPool": "pool",
  "SchedulesThreadPool": "devpool",
  "Bus": "bus",
  "DeviceDownTimeout": 60,
  "AllowedFails": 5,
  "Driver": {
     "LDSName": "LDS"
  }
}
