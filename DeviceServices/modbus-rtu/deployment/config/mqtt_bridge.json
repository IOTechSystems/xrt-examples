{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "Patterns": [{"Pattern":"xrt/devices/modbus/telemetry","Retain": true, "Qos": 1},
               {"Pattern":"xrt/devices/modbus/reply","Retain": true, "Qos": 1},
               {"Pattern":"xrt/devices/modbus/discovery","Retain": true, "Qos": 1},
               {"Pattern":"xrt/devices/modbus/status","Retain": true, "Qos": 1}],
  "MQTTPatterns": [{"Pattern":"xrt/devices/modbus/request","Sync": true}],
  "MQTTConfig":
  {
    "ServerURI": "${XRT_MQTT_BROKER}",
    "ClientID": "xrt",
    "QoS": 1,
    "ClientConfig":
    {
      "KeepAliveInterval": 60,
      "MQTTVersion": 5,
      "RetryInterval": 5,
      "Username": "${XRT_MQTT_USERNAME}",
      "Password": "${XRT_MQTT_PASSWORD}",
      "MaxBufferedMessages": 100
    }
  },
  "Cookie": 125,
  "QueueSize": 5,
  "Bus": "bus",
  "Logger": "logger"
}
