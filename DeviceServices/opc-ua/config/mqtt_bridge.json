{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "Patterns": [ 
    "xrt/device/opc_ua_device_service/data",
    "xrt/+/opc_ua_device_service/reply",
    "xrt/discovery/opc_ua_device_service/devices"
  ],
  "MQTTPatterns": [ "xrt/+/opc_ua_device_service/request" ],
  "MQTTConfig":
  {
    "ServerURI": "${MQTT_SERVER_URI}",
    "ClientID": "xrt",
    "QoS": 1,
    "ClientConfig": {"Username":"test","Password": "tube","KeepAliveInterval": 10, "MQTTVersion": 5, "Reliable": true},
    "RetryCount": 5,
    "RetryInterval": 5
  },
  "Cookie": 125,
  "QueueSize": 5,
  "Bus": "bus",
  "Logger": "logger"
}
