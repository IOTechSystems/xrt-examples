{
  "Library": "libxrt-zigbee-device-service.so",
  "Factory": "xrt_zigbee_device_service_factory",
  "Name":"zigbee",
  "ProfileDir":"${XRT_STATE_DIR}/profiles",
  "StateDir":"${XRT_STATE_DIR}",
  "Group":"${SPARKPLUG_GROUP}",
  "Scheduler":"5-sched",
  "Logger":"11-logger_debug",
  "ThreadPool":"3-pool",
  "SchedulesThreadPool": "4-devpool",
  "Bus":"6-bus",
  "QueueMax": 100,
  "AutoRegister": false,
  "DiscoveryTopic": "spBv1.0/${SPARKPLUG_GROUP}/zigbee/discovery",
  "Driver": {
    "BaseZ2MQTTTopic": "zigbee2mqtt/"
  }
}
