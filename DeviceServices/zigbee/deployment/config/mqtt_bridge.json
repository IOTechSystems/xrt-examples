{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "Patterns": [{"Pattern":"xrt/devices/zigbee/telemetry","Retain": true, "Qos": 1},
               {"Pattern":"xrt/devices/zigbee/reply","Retain": true, "Qos": 1},
               {"Pattern":"xrt/devices/zigbee/discovery","Retain": true, "Qos": 1},
               {"Pattern":"zigbee2mqtt/+/get","Retain": true, "Qos": 1},
               {"Pattern":"zigbee2mqtt/+/set","Retain": true, "Qos": 1},
               {"Pattern":"zigbee2mqtt/bridge/request/#","Retain": true, "Qos": 1}],
  "MQTTPatterns": [{"Pattern":"xrt/devices/zigbee/request", "Sync": true},
                   {"Pattern":"zigbee2mqtt/+", "Sync": true},
                   {"Pattern":"zigbee2mqtt/bridge/response/#", "Sync": true},
                   {"Pattern":"zigbee2mqtt/bridge/devices", "Sync": true}],
  "MQTTConfig":
  {
    "ServerURI": "${XRT_MQTT_BROKER}",
    "ClientID": "xrt",
    "QoS": 1,
    "ClientConfig":
    {
      "KeepAliveInterval": 60,
      "MQTTVersion": 5,
      "Reliable": true,
      "Username": "${XRT_MQTT_USERNAME}",
      "Password": "${XRT_MQTT_PASSWORD}"
    },
    "RetryCount": 5,
    "RetryInterval": 5
  },
  "Cookie": 125,
  "QueueSize": 5,
  "Bus": "bus",
  "Logger": "logger"
}
