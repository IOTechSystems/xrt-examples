{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "Patterns":
  [
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/DBIRTH/${SPARKPLUG_NODE}/+", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/DDEATH/${SPARKPLUG_NODE}/+", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/DDATA/${SPARKPLUG_NODE}/+", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/NBIRTH/${SPARKPLUG_NODE}", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/NDEATH/${SPARKPLUG_NODE}", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/NDATA/${SPARKPLUG_NODE}", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/REPLY/${SPARKPLUG_NODE}/zigbee",  "QoS":0}
    { "Pattern":"zigbee2mqtt/+/get","Retain": false, "Qos": 1},
    { "Pattern":"zigbee2mqtt/+/set","Retain": false, "Qos": 1},
    { "Pattern":"zigbee2mqtt/bridge/request/#","Retain": false, "Qos": 1}
  ],
  "MQTTPatterns": 
  [
     { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/STATE/+", "Qos": 0, "Sync":true },
     { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/NCMD/${SPARKPLUG_NODE}", "Qos": 1, "Sync":true, "Transforms": ["${SPARKPLUG_PROTO}"]  },
     { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/REQUEST/${SPARKPLUG_NODE}/zigbee", "Qos": 0, "Sync":true }
     { "Pattern":"zigbee2mqtt/+", "Qos": 1, "Sync": true},
     { "Pattern":"zigbee2mqtt/bridge/response/#", "Qos": 1, "Sync": true},
     { "Pattern":"zigbee2mqtt/bridge/devices", "Qos": 1, "Sync": true}
  ],
  "MQTTDynamicPatterns":
  [
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/DCMD/${SPARKPLUG_NODE}/+", "QoS":1, "Sync":true, "Transforms": ["${SPARKPLUG_PROTO}"] }
  ],
  "MQTTAppId": "XRT::MQTTBridge",
  "MQTTConfig":
  {
    "ServerURI": "${XRT_MQTT_BROKER}",
    "ClientID": "${SPARKPLUG_NODE}",
    "QoS": 1,
    "ClientConfig":
    {
      "KeepAliveInterval": 60,
      "MQTTVersion": 5,
      "Reliable": true,
      "Username": "${XRT_MQTT_USERNAME}",
      "Password": "${XRT_MQTT_PASSWORD}",
      "MaxBufferedMessages": 100,
      "WillConfig":
      {
        "Topic": "spBv1.0/${SPARKPLUG_GROUP}/NDEATH/${SPARKPLUG_NODE}",
        "Transforms": ["${SPARKPLUG_PROTO}"],
        "QoS": 1,
        "Retain": false
      }
    },
  },
  "QueueSize": 5,
  "Bus": "6-bus",
  "Logger": "10-logger_debug"
}
