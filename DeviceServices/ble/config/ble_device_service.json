{
  "Library": "libxrt-ble-device-service.so",
  "Factory": "xrt_ble_device_service_factory",
  "Name": "ble_device_service",
  "Topic": "xrt/device/ble_device_service/data",
  "RequestTopic": "xrt/device/ble_device_service/request",
  "ReplyTopic": "xrt/device/ble_device_service/reply",
  "DiscoveryRequestTopic": "xrt/discovery/ble_device_service/request",
  "DiscoveryReplyTopic": "xrt/discovery/ble_device_service/reply",
  "DiscoveryTopic": "xrt/discovery/ble_device_service/devices",
  "ProfileRequestTopic": "xrt/profile/ble_device_service/request",
  "ProfileReplyTopic": "xrt/profile/ble_device_service/reply",
  "ScheduleRequestTopic": "xrt/schedule/ble_device_service/request",
  "ScheduleReplyTopic": "xrt/schedule/ble_device_service/reply",
  "DiscoveryInterval": 60,
  "ProfileDir":"${XRT_PROFILE_DIR}",
  "StateDir":"${XRT_STATE_DIR}",
  "Scheduler": "sched",
  "Logger": "logger",
