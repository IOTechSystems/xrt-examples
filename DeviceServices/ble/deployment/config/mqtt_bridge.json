{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "Patterns": [{"Pattern": "xrt/devices/ble/telemetry", "Retain": false, "Qos": 1},
               {"Pattern": "xrt/devices/ble/reply", "Retain": false, "Qos": 1},
               {"Pattern": "xrt/devices/ble/discovery", "Retain": false, "Qos": 1},
               {"Pattern": "xrt/devices/ble/status", "Retain": false, "Qos": 1}],
  "MQTTPatterns": [{"Pattern":"xrt/devices/ble/request", "Sync": true}],
  "MQTTConfig":
  {
    "ServerURI": "${XRT_MQTT_BROKER}",
    "ClientID": "xrt",
    "QoS": 1,
    "ClientConfig":
    {
      "KeepAliveInterval": 60,
      "MQTTVersion": 5,
      "RetryInterval": 5,
      "Username": "${XRT_MQTT_USERNAME}",
      "Password": "${XRT_MQTT_PASSWORD}",
      "MaxBufferedMessages": 100
    }
  },
  "Cookie": 125,
  "QueueSize": 5,
  "Bus": "bus",
  "Logger": "logger"
}
