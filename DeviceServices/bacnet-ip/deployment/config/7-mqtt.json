{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "Patterns":
  [
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/DBIRTH/#", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/DDEATH/#", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/DDATA/#", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/NBIRTH/${SPARKPLUG_NODE}", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/NDEATH/${SPARKPLUG_NODE}", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/NDATA/${SPARKPLUG_NODE}", "QoS":0, "Transforms": ["${SPARKPLUG_PROTO}"] },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/REPLY/${SPARKPLUG_NODE}/bacnet_ip" }
  ],
  "MQTTPatterns":
  [
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/DCMD/#", "Sync":true, "Transforms": ["${SPARKPLUG_PROTO}"]  },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/NCMD/${SPARKPLUG_NODE}", "Sync":true, "Transforms": ["${SPARKPLUG_PROTO}"]  },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/STATE/+", "Sync":true },
    { "Pattern":"spBv1.0/${SPARKPLUG_GROUP}/REQUEST/${SPARKPLUG_NODE}/bacnet_ip", "Sync":true }
  ],
  "MQTTConfig":
  {
    "ServerURI": "${XRT_MQTT_BROKER}",
    "ClientID": "${SPARKPLUG_NODE}",
    "QoS": 0,
    "ClientConfig":
    {
      "Username":"${XRT_MQTT_USERNAME}",
      "Password": "${XRT_MQTT_PASSWORD}",
      "KeepAliveInterval": 10,
      "MQTTVersion": 5,
      "Reliable": true,
      "WillConfig":
      {
        "Topic": "spBv1.0/${SPARKPLUG_GROUP}/NDEATH/${SPARKPLUG_NODE}",
        "Transforms": ["${SPARKPLUG_PROTO}"],
        "QoS": 1,
        "Retain": false
      }
    }
  },
  "Cookie": 125,
  "Bus": "6-bus",
  "QueueSize": 5,
  "Logger": "10-logger_debug"
}
