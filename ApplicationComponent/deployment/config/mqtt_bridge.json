{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "MQTTConfig":
  {
    "ServerURI": "127.0.0.1",
    "ClientID": "mqtt_export",
    "MQTTInstance": 0,
    "QoS": 1,
    "ClientConfig":
    {
      "KeepAliveInterval": 60,
      "MQTTVersion": 5,
      "ConnectTimeout": 0,
      "CleanSession": false,
      "CleanStart": false
    }
  },
  "Bus": "bus",
  "Logger": "logger_debug",
  "Patterns": ["virtual_device_service/result"],
  "Topic": "virtual_device_service/final_result",
  "QueueSize": 4
}

