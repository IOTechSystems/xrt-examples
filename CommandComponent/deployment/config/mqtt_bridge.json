{
  "Library": "libxrt-mqtt-bridge.so",
  "Factory": "xrt_mqtt_bridge_factory",
  "Patterns": [{"Pattern":"xrt/command/reply","Retain": true, "Qos": 1},
               {"Pattern":"xrt/devices/virtual/telemetry","Retain": true, "Qos": 1},
               {"Pattern":"xrt/devices/virtual/reply","Retain": true, "Qos": 1},
               {"Pattern":"xrt/devices/virtual/discovery","Retain": true, "Qos": 1}],
  "MQTTPatterns": [{"Pattern":"xrt/command/request", "Sync": true},
                   {"Pattern":"xrt/devices/virtual/request", "Sync": true}],
  "MQTTConfig":
  {
    "ServerURI": "${XRT_MQTT_BROKER}",
    "ClientID": "xrt",
    "QoS": 1,
    "ClientConfig":
    {
      "KeepAliveInterval": 60,
      "MQTTVersion": 5,
      "RetryInterval": 5,
      "Username": "${XRT_MQTT_USERNAME}",
      "Password": "${XRT_MQTT_PASSWORD}"
    },
  },
  "Cookie": 125,
  "QueueSize": 5,
  "Bus": "bus",
  "Logger": "logger"
}
